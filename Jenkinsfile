pipeline {
    agent any

    stages {
        stage('BUILD') {
            steps {
                echo 'Hello World'
            }
        }
         stage('TEST') {
            steps {
             
                echo 'Hello World' > file1.txt
             
            }
        }
         stage('DEPLOY') {
            steps {
              
                echo 'Hello World' > file
             
            }
        }
    }
}
